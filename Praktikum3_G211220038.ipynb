      "source": [
        "# Program Python untuk mengonversi daftar karakter menjadi string\n",
        "\n",
        "def convert(s):\n",
        "    new = \"\"\n",
        "    for x in s:\n",
        "        new += x\n",
        "    return new\n",
        "\n",
        "# driver code\n",
        "s = ['p', 'r', 'o', 'g', 'r', 'a', 'm', 'm', 'i', 'n', 'g']\n",
        "print(convert(s))\n"
      ]
    }
  ]
